  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/train.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceRoot}"
      },
      "args": [
        "-c",
        "./config.json"
      ]
    },
    {
      "name": "Run this",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceRoot}"
      },
      "args": [
        "-c",
        "./config.json",
        "--train"
      ]
    }
  ]
}